{
	"name": "nodejs-analytics-starter",
	"version": "1.0.0",
	"description": "A starting point for Node.js express apps with TypeScript",
	"repository": {
		"type": "git",
		"url": "https://github.com/hta218/nodejs-analytics-starter"
	},
	"author": {
		"name": "Tuan Anh Huynh",
		"email": "huynhtuananh21895@gmail.com"
	},
	"license": "MIT",
	"scripts": {
		"start": "npm run serve",
		"build": "npm run build-ts --scripts-prepend-node-path && npm run copy-static-assets",
		"serve": "yarn build && NODE_ENV=production node dist/server.js",
		"watch-node": "nodemon dist/server.js",
		"watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
		"test": "jest --forceExit --coverage --verbose",
		"watch-test": "npm run test -- --watchAll",
		"build-ts": "tsc",
		"watch-ts": "tsc -w",
		"lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
		"copy-static-assets": "ts-node copyStaticAssets.ts --scripts-prepend-node-path",
		"debug": "npm run build && npm run watch-debug",
		"serve-debug": "nodemon --inspect dist/server.js",
		"watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
		"deploy": "yarn build && NODE_ENV=production pm2 start dist/server.js --name=PFAnalytic -i max",
		"restart": "yarn build && NODE_ENV=production pm2 restart dist/server.js --name=PFAnalytic -i max"
	},
	"dependencies": {
		"body-parser": "1.19.0",
		"compression": "1.7.4",
		"connect-mongo": "3.2.0",
		"date-fns": "2.11.0",
		"dotenv": "8.2.0",
		"errorhandler": "1.5.1",
		"express": "4.17.3",
		"express-session": "1.17.0",
		"mongoose": "5.13.15",
		"morgan": "^1.10.0",
		"winston": "3.2.1"
	},
	"devDependencies": {
		"@types/body-parser": "1.19.0",
		"@types/compression": "1.7.0",
		"@types/concurrently": "5.1.0",
		"@types/connect-mongo": "3.1.3",
		"@types/errorhandler": "1.5.0",
		"@types/eslint": "6.1.8",
		"@types/express": "4.17.3",
		"@types/express-session": "1.17.0",
		"@types/mongoose": "5.7.7",
		"@types/node": "13.9.3",
		"@types/shelljs": "0.8.7",
		"@types/winston": "2.4.4",
		"@typescript-eslint/eslint-plugin": "2.24.0",
		"@typescript-eslint/parser": "2.24.0",
		"concurrently": "5.1.0",
		"eslint": "6.8.0",
		"jest": "25.1.0",
		"node-sass": "7.0.0",
		"nodemon": "2.0.20",
		"shelljs": "0.8.5",
		"ts-jest": "25.2.1",
		"ts-node": "8.8.1",
		"typescript": "3.8.3"
	}
}
